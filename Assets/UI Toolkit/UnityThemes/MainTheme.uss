    border-top-color: rgba(48, 48, 48, 0.39);
    border-bottom-color: rgba(36, 36, 36, 0.39);
    padding-left: 100px;
    padding-right: 100px;
    width: 300px;
    color: rgb(220, 220, 220);
}

